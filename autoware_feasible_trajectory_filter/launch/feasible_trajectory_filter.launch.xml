<launch>
  <arg name="input"/>
  <arg name="output"/>

  <node pkg="autoware_feasible_trajectory_filter" exec="autoware_feasible_trajectory_filter_node" name="feasible_trajectory_filter" output="screen">
    <remap from="~/input/trajectories" to="$(var input)"/>
    <remap from="~/output/trajectories" to="$(var output)"/>
  </node>
</launch>
